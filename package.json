{
  "name": "@sylchi/reusable-components",
  "version": "0.0.4",
  "description": "Reusable component library",
  "private": false,
  "main": "dist/index.js",
  "module": "dist/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "prepublish": "rm -rf ./dist && npm run build",
    "build": "rm -rf dist && NODE_ENV=production babel --extensions .ts,.tsx ./src --out-dir dist --copy-files"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "peerDependencies": {
    "react": ">=17",
    "react-dom": ">=17"
  },
  "devDependencies": {
    "@babel/cli": "^7.21.0",
    "@babel/core": "^7.21.4",
    "@babel/preset-env": "^7.21.4",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.21.4",
    "@types/react": "^18.0.31",
    "react": ">=17",
    "react-dom": ">=17"
  },
  "dependencies": {
    "@babel/polyfill": "^7.12.1",
    "core-js": "3.29.1"
  },
  "babel": {
    "presets": [
      [
        "@babel/env",
        {
          "targets": {
            "edge": "17",
            "firefox": "60",
            "chrome": "67",
            "safari": "11.1"
          },
          "useBuiltIns": "usage",
          "corejs": "3.29.1"
        }
      ],
      [ "@babel/preset-react", {"runtime": "automatic"}],
      [
        "@babel/preset-typescript",
        {
          "isTSX": true,
          "allExtensions": true
        }
      ]
    ]
  }
}
