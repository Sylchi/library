{
  "name": "@sylchi/reusable-components",
  "version": "0.2.3",
  "description": "Reusable component library",
  "private": false,
  "source": "src/index.ts",
  "main": "dist/cjs/index.cjs",
  "module": "dist/esm/index.mjs",
  "types": "dist/types.d.ts",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Sylchi/library.git"
  },
  "scripts": {
    "watch": "parcel watch",
    "build": "rm -rf dist && parcel build && npm pkg delete devDependencies"
  },
  "browserslist": "> 0.5%, last 2 versions, not dead",
  "targets": {
    "main": {
      "context": "node",
      "engines": {
        "node": ">= 14"
      },
      "isLibrary": true
    },
    "module": {
      "context": "browser",
      "isLibrary": true
    }
  },
  "engines": {
    "node": ">= 14"
  },
  "keywords": [],
  "author": "Ken Kauksi",
  "license": "ISC",
  "peerDependencies": {
    "react": ">=17"
  },
  "devDependencies": {
    "@parcel/packager-ts": "^2.8.3",
    "@parcel/transformer-typescript-types": "^2.8.3",
    "@types/react": "^18.0.31",
    "@types/react-dom": "^18.0.11",
    "parcel": "^2.8.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^5.0.3"
  }
}
